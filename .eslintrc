{
  "rules": {
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "never"
    ],
    "no-console": [0],
    "comma-dangle": [0],
    "new-cap": [0],
    "react/jsx-indent-props": [0],
    "react/jsx-space-before-closing": [0],
    "react/prefer-stateless-function": [0],
    "max-len": [1, 120],
    "object-curly-spacing": [0]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "airbnb",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "modules": true
  },
  "parser": "Babel-ESLint",
  "plugins": [
    "react"
  ]
}
